# Mapserver file

MAP
  IMAGETYPE      JPEG
  SIZE           2100 1600
  IMAGECOLOR     255 255 255
  FONTSET        "fonts.list"

  OUTPUTFORMAT
    NAME "aggjpg24"
    DRIVER AGG/JPEG
    MIMETYPE "image/jpeg"
    IMAGEMODE RGB
    EXTENSION "jpg"
  END

  SYMBOL
    NAME "point"
    TYPE ELLIPSE
    POINTS 1 1 END
    FILLED TRUE
  END

  UNITS dd
  PROJECTION
    "proj=longlat"
    "ellps=WGS84"
    "datum=WGS84"
    "no_defs"
  END

  LEGEND
    KEYSIZE 36 36
    LABEL
      SIZE 24
      TYPE TRUETYPE
      FONT opensymbol
      ANTIALIAS TRUE
      OUTLINECOLOR 255 255 255
      OUTLINEWIDTH 3
      COLOR 0 0 89
      POSITION cc
    END
    STATUS EMBED
    POSITION lr
    IMAGECOLOR 255 255 255
    TRANSPARENT FALSE
  END

  SCALEBAR
    LABEL
      SIZE 20
      TYPE TRUETYPE
      FONT opensymbol
      ANTIALIAS TRUE
      COLOR 0 0 0
    END
    STYLE 0
    SIZE 600 25
    IMAGECOLOR 255 255 255
    OUTLINECOLOR 0 0 0
    TRANSPARENT FALSE
    COLOR 0 0 0
    UNITS MILES
    INTERVALS 5
    STATUS EMBED
    POSITION ll
  END

  # Information about the mode=2 option
  # https://gis.stackexchange.com/questions/26958/gdalinfo-gives-no-georeferencing-information-for-a-postgis-raster
  LAYER
    NAME aerial
    DATA "PG:host=/var/run/postgresql/ dbname='__DBNAME__' table='wv_aerial_usda_2014' mode=2"
    OFFSITE 0 0 0
    STATUS ON
    TYPE RASTER
    PROJECTION
      "+proj=merc"
      "+a=6378137"
      "+b=6378137"
      "+lat_ts=0.0"
      "+lon_0=0.0"
      "+x_0=0.0"
      "+y_0=0"
      "+k=1.0"
      "+units=m"
      "+nadgrids=@null"
      "+wktext"
      "+no_defs"
    END
  END

